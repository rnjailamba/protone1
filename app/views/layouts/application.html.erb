<!DOCTYPE html>
<html>
<head>
  <title>Protone1</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  
  

  <%= csrf_meta_tags %>
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  
  <script type="text/javascript" src="http://maps.google.com/maps/api/js?sensor=false&libraries=places&language=en-US"></script>
  
  <link rel="stylesheet" href="//maxcdn.bootstrapcdn.com/font-awesome/4.3.0/css/font-awesome.min.css">
</head>
<body>
  <%= render "layouts/header" unless @skip_header %>
  <div class="flash-message">
    <% flash.each do |name, msg| %>
       <%= content_tag(:div, msg, class: "alert alert-success alert-block fade in alert-dismissible") %>
    <% end %>
  </div>

    <%= yield %>

  <%= render "layouts/footer" unless @skip_footer %>

</body>
</html>
